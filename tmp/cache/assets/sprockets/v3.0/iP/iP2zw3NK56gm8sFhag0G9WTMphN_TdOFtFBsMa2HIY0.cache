     <script>
       //basic map config with custom fills, mercator projection
      var map = new Datamap({
        scope: 'world',
        element: document.getElementById('container1'),
        projection: 'orthographic',
        fills: {
          defaultFill: 'rgba(0,0,0,0.7)',
          lt50: 'rgba(0,244,244,0.9)',
          gt50: 'red'
        },
        projectionConfig: {
          rotation: [97,-30]
        },
        data: {
          '071': {fillKey: 'lt50' },
          '001': {fillKey: 'gt50' }       
        }
      });

      map.graticule();

      map.arc([{
        origin: {
          latitude: 61,
          longitude: -149
        },
        destination: {
          latitude: -22,
          longitude: -43
        }
      }], {
        greatArc: true,
        animationSpeed: 2000
      });

      
     </script>
