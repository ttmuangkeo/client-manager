    "test:typecheck": "tsc --noEmit --incremental",
    "test:unit": "jest --config config/jest.json",
    "coveralls": "npm run test:unit -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
