    "glob": "^7.1.6",
    "jest": "^27.5.1",
    "npm-run-all": ">=4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "1.1.0",
