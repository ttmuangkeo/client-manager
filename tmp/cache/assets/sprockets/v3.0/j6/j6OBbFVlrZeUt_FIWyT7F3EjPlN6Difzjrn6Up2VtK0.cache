        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/fb55/entities.git"
    },
    "license": "BSD-2-Clause",
    "jest": {
        "preset": "ts-jest",
        "coverageProvider": "v8",
        "moduleNameMapper": {
            "^(.*)\\.js$": "$1"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "proseWrap": "always"
    }
}
