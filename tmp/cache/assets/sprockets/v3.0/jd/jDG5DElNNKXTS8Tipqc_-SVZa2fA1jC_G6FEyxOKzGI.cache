    "@types/yargs": "^13.0.0",
    "eslint": "^7.14.0",
    "husky": "^3.0.0",
    "jest": "^24.1.0",
    "prettier": "^2.2.0",
    "semantic-release": "^17.2.4",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.1.2"
  }
}
